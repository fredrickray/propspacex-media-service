{
  "name": "propspacex-media-service",
  "version": "1.0.0",
  "description": "Media service for PropSpaceX microservice architecture",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "build:only": "tsc && tsc-alias",
    "build": "npm run clean && npm run build:only",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "media",
    "microservice",
    "grpc",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.964.0",
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "cloudinary": "^1.41.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "eslint": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fredrickray/propspacex-media-service.git"
  },
  "bugs": {
    "url": "https://github.com/fredrickray/propspacex-media-service/issues"
  },
  "homepage": "https://github.com/fredrickray/propspacex-media-service#readme"
}
